<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ScrollPane?>
<ScrollPane  xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cafe94.StaffDashViewBookingsController">


         <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0">
             <fx:include source="page.fxml" />
            <padding>
               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
            <children>
               <Label fx:id="welcomeText" alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="220.0" text="Welcome Text" />
            <TableView id="table" fx:id="table" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="id" prefWidth="75.0" text="Id" />
                  <TableColumn fx:id="customerName" prefWidth="75.0" text="Customer Name" />


                  <TableColumn fx:id="tableId" prefWidth="75.0" text="Table Id" />
                  <TableColumn fx:id="tableSeats" prefWidth="75.0" text="Table Seats" />


                <TableColumn fx:id="date" prefWidth="75.0" text="Date" />
                <TableColumn fx:id="time" prefWidth="75.0" text="Time" />
                <TableColumn fx:id="approved" prefWidth="75.0" text="Approved" />
              </columns>
            </TableView>


            </children>
         </VBox>

</ScrollPane>
