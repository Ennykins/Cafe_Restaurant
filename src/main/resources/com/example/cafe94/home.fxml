<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cafe94.HomeController">

   <children>
         <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0">

            <padding>
               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
            <children>
               <Label fx:id="welcomeText1" prefHeight="30.0" prefWidth="85.0" text="Choose Mode" />
               <Button onAction="#switchToCustomerLogin" text="Customer" />
               <Button onAction="#switchToManagerLogin" text="Staff" />
            </children>
         </VBox>
      </children>
</AnchorPane>
