<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ScrollPane?>
<ScrollPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cafe94.StaffDashController">


         <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0">
            <fx:include source="page.fxml" />
            <padding>
               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
            <children>
               <Label fx:id="welcomeText" alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="220.0" text="Welcome Text" />
               <Button onAction="#viewCustomers" text="View Customers" />
               <Button onAction="#viewStaffs" text="View Staffs" />
               <Button onAction="#createStaff" text="Create Staff" />

               <Button onAction="#viewBookings" text="View Bookings" />
               <Button onAction="#viewOrders" text="View Orders" />
               <Button onAction="#viewMenus" text="View Menus" />
               <Button onAction="#createMenu" text="Create Menu" />
            </children>
         </VBox>

</ScrollPane>
